{
  "require": {
    "craftcms/cms": "3.0.24",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/redactor": "2.1.6",
    "benjamminf/craft-embedded-assets": "1.0.2",
    "aelvan/imager": "v2.1.0",
    "topshelfcraft/wordsmith": "3.0.2",
    "ether/seo": "3.4.4",
    "studioespresso/craft-navigate": "1.1.3",
    "olivestudio/craft-olivemenus": "1.0.0",
    "xpertbot/craft-wheelform": "v1.12.0",
    "carlcs/craft-redactorcustomstyles": "v3.0.2",
    "verbb/redactor-tweaks": "2.0.2",
    "verbb/field-manager": "2.0.2",
    "rias/craft-colour-swatches": "1.1.4"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
